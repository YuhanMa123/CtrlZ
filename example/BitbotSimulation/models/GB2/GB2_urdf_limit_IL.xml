                            <joint name="left_knee_joint" pos="0 0 0" axis="0 1 0" limited="true"
                                range="0.09 1.9" />
                            <geom size="0.05 0.15" pos="0 0 -0.225" type="cylinder"
                                rgba="0.752941 0.752941 0.752941 1" />
                            <body name="left_ankle_pitch_link" pos="0 0 -0.45">
                                <inertial pos="-0.002774 0.000432 -0.006398"
                                    quat="0.438819 0.554471 0.554471 0.438819" mass="0.099238"
                                    diaginertia="2.5e-05 2.42361e-05 1.97639e-05" />
                                <joint name="left_ankle_pitch_joint" pos="0 0 0" axis="0 1 0"
                                    limited="true" range="-1.1 0.9" />
                                <body name="left_ankle_roll_link" pos="0 0 -0.015">
                                    <inertial pos="0.01803 -0.000161 -0.037272"
                                        quat="0.000822343 0.670875 0.00521497 0.741552"
                                        mass="1.26562"
                                        diaginertia="0.00572126 0.00538116 0.00117958" />
                                    <joint name="left_ankle_roll_joint" pos="0 0 0" axis="1 0 0"
                                        limited="true" range="-0.314 0.314" />
                                    <geom type="mesh" rgba="0.752941 0.752941 0.752941 1"
                                        mesh="lankle2" />
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
            <body name="left_shoulder_pitch_link" pos="-0.026283 0.136 0.391">
                <inertial pos="1.7e-05 0.08658 -0.122198"
                    quat="0.999964 -0.00837945 6.62209e-05 0.00141515" mass="0.955826"
                    diaginertia="0.019115 0.0191032 0.000721837" />
                <joint name="left_shoulder_pitch_joint" pos="0 0 0" axis="0 1 0" limited="true"
