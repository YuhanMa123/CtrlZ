<mujoco model="GB2_urdf_limit">
    <compiler angle="radian" meshdir="meshes/" />
    <size njmax="500" nconmax="100" />

    <default>
        <joint limited='true' />
        <motor ctrllimited='true' />
        <geom condim='4' contype="1" conaffinity="15" solref='0.001 2' friction='0.9 0.2 0.2'
        />
        <equality solref='0.001 2' />
        <default class='visualgeom'>
            <geom material='visualgeom' condim='1' contype='0' conaffinity='0' />
        </default>
        <default class='visualgeom2'>
            <geom material='visualgeom2' condim='1' contype='0' conaffinity='0' />
        </default>
        <default class='obstacle'>
            <geom material='obstacle' condim='3' contype='1' conaffinity='15' />
        </default>
        <default class='leg_joint_param'>
            <joint damping="0.01" frictionloss="0.01" armature="0.01" />
            <!-- <joint damping="0.0" frictionloss="0.0" armature="0.0"/> -->
        </default>
    </default>
    <asset>
        <!-- Set ground -->
        <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3"
            rgb2=".2 .3 .4" />
        <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2" />
    </asset>

    <option timestep="0.001" integrator="RK4">
        <flag sensornoise="enable" frictionloss="enable" />
    </option>

    <visual>
        <map force="0.1" zfar="30" />
        <rgba haze="0.15 0.25 0.35 1" />
        <global offwidth="2560" offheight="1440" elevation="-30" azimuth="120" />
    </visual>

    <asset>
        <mesh name="torso" file="torso.stl" />

        <mesh name="lhip1" file="lhip1.stl" />
        <mesh name="lhip2" file="lhip2.stl" />
        <mesh name="lhip3" file="lhip3.stl" />
        <mesh name="lknee" file="lknee.stl" />
        <mesh name="lankle1" file="lankle1.stl" />
        <mesh name="lankle2" file="lankle2.stl" />
        <mesh name="lshoulder" file="lshoulder.stl" />

        <mesh name="rhip1" file="rhip1.stl" />
        <mesh name="rhip2" file="rhip2.stl" />
        <mesh name="rhip3" file="rhip3.stl" />
        <mesh name="rknee" file="rknee.stl" />
        <mesh name="rankle1" file="rankle1.stl" />
        <mesh name="rankle2" file="rankle2.stl" />
        <mesh name="rshoulder" file="rshoulder.stl" />
    </asset>
    <worldbody>
        <!-- Main Light Source -->
        <light name="main_light" diffuse="2 2 2" specular="0.5 0.5 0.5" pos="0 0 40" dir="0 0 -1"
            castshadow="true" />
        <!-- Auxiliary Light Sources-->
        <light name="aux_light1" diffuse=".8 .8 .8" pos=" 10 0 1" dir="-1 0 0" castshadow="false" />
        <light name="aux_light2" diffuse=".8 .8 .8" pos="-10 0 1" dir=" 1 0 0" castshadow="false" />
        <light name="aux_light3" diffuse=".8 .8 .8" pos="0  10 1" dir="0 -1 0" castshadow="false" />
        <light name="aux_light4" diffuse=".8 .8 .8" pos="0 -10 1" dir="0  1 0" castshadow="false" />
        <!-- Floor -->
        <!-- <geom name="floor" type="plane" size="0 0 0.05" material="grid"/> -->
        <geom name="floor" type="plane" size="10 10 0.05" material="grid" />
        <body name="torso" pos="0 0 1.10">
            <freejoint name="torso" />
            <inertial pos="-0.010072 -0.001093 0.191269" mass="24.987113"
                fullinertia="0.580926 0.423278 0.279351 0.001305 -0.044142 -0.000670 " />
            <geom size="0.135 0.16 0.25" pos="-0.03 0 0.225" type="box"
                rgba="0.752941 0.752941 0.752941 1" />
            <site name="imu_site" pos="0 0 0" size="0.02" rgba="1 0 0 1" type="sphere" />
            <!-- <site name='imu' size='0.01' pos='0.0 0 0.0' /> -->

            <body name="left_hip_yaw_link" pos="-3.2657e-05 0.1 -0.011">
                <inertial pos="-0.078509 -4e-06 -0.081791"
                    quat="0.312936 0.634303 0.633788 0.313119"
                    mass="2.38958" diaginertia="0.011987 0.0111943 0.00318774" />
                <joint name="left_hip_yaw_joint" pos="0 0 0" axis="0 0 1" limited="true"
                    range="-0.5 0.5" />
                <body name="left_hip_roll_link" pos="0.001 0 -0.103">
                    <inertial pos="-0.003666 0.001437 2e-06"
                        quat="0.0151806 0.706944 -0.0151806 0.706944" mass="2.493025"
                        diaginertia="0.003755 0.00345456 0.00315144" />
                    <joint name="left_hip_roll_joint" pos="0 0 0" axis="1 0 0" limited="true"
                        range="-0.25 0.5" />
                    <body name="left_hip_pitch_link" pos="0 0 0">
                        <inertial pos="0.000491 0.002117 -0.216944"
