                    quat="0.312936 0.634303 0.633788 0.313119"
                    mass="2.38958" diaginertia="0.011987 0.0111943 0.00318774" />
                <joint name="right_hip_yaw_joint" pos="0 0 0" axis="0 0 1" limited="true"
                    range="-0.5 0.5" />
                <body name="right_hip_roll_link" pos="0.001 0 -0.103">
                    <inertial pos="-0.003613 -0.002804 -4.9e-05"
                        quat="-0.0339114 0.708501 0.0418884 0.703649" mass="2.49302"
                        diaginertia="0.00398908 0.0035721 0.00338382" />
                    <joint name="right_hip_roll_joint" pos="0 0 0" axis="1 0 0" limited="true"
                        range="-0.5 0.25" />
                    <body name="right_hip_pitch_link" pos="0 0 0">
                        <inertial pos="0.000491 -0.002117 -0.216944"
                            quat="0.999953 -0.00971676 -0.000638381 0.000192479" mass="2.46673"
                            diaginertia="0.0607601 0.0589917 0.00685222" />
                        <joint name="right_hip_pitch_joint" pos="0 0 0" axis="0 1 0" limited="true"
                            range="-1.57 0.8" />
                        <geom size="0.05 0.175" pos="0 0 -0.24" type="cylinder"
                            rgba="0.752941 0.752941 0.752941 1" />
                        <body name="right_knee_link" pos="0 -0.0001 -0.4515">
                            <inertial pos="-0.003764 -0.00093 -0.229728"
                                quat="0.999323 -0.00545204 -0.0262826 -0.0251738" mass="2.69388"
