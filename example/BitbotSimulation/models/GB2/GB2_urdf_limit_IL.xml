                            diaginertia="0.060760 0.058972 0.006872" />
                        <joint name="left_hip_pitch_joint" pos="0 0 0" axis="0 1 0" limited="true"
                            range="-1.57 0.8" />
                        <geom size="0.05 0.175" pos="0 0 -0.24" type="cylinder"
                            rgba="0.752941 0.752941 0.752941 1" />
                        <body name="left_knee_link" pos="0 0.0001 -0.4515">
                            <inertial pos="-0.003765 -0.00093 -0.229728"
